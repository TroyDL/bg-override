{
    "manifest_version": 2,

    "name": "Override Background Color",
    "description": "Override the background color of all page elements.",
    "version": "0.13.7.31",

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "css": ["style.css"],
            "all_frames": true
        }
    ],
    "permissions": [
        "tabs", "*://*/*"
    ]
}
